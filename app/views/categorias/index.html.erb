<h1>Listing categorias</h1>

<table>
  <tr>
    <th>Admin</th>
    <th>Descricao</th>
    <th>Subcat</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @categorias.each do |categoria| %>
  <tr>
    <td><%= categoria.admin_id %></td>
    <td><%= categoria.id %> - <%= categoria.descricao %></td>
    <td><%= categoria.subcat_id %></td>
    <td><%= link_to 'Show', categoria %></td>
    <td><%= link_to 'Edit', edit_categoria_path(categoria) %></td>
    <td><%= link_to 'Destroy', categoria, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Categoria', new_categoria_path %>
